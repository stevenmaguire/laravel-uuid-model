{
    "name": "stevenmaguire/laravel-uuid-model",
    "description": "Create non-incrementing Laravel models whose primary key is a UUID",
    "keywords": [
        "stevenmaguire",
        "laravel",
        "eloquent",
        "model",
        "orm",
        "guid",
        "uuid"
    ],
    "homepage": "https://github.com/stevenmaguire/laravel-uuid-model",
    "license": "MIT",
    "authors": [
        {
            "name": "Steven Maguire",
            "email": "stevenmaguire@gmail.com",
            "homepage": "http://stevenmaguire.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "illuminate/database": "^5.0",
        "ramsey/uuid": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Stevenmaguire\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stevenmaguire\\Laravel\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
